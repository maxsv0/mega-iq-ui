{
  "name": "mega-iq-ui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n mega-iq-ui --i18n-format xlf2 --output-path i18n --i18n-locale en && ng run mega-iq-ui:xliffmerge",
    "start-de": "ng serve --configuration=de",
    "start-ru": "ng serve --configuration=ru",
    "start-es": "ng serve --configuration=es",
    "build:prod:en": "ng build --configuration=production-en",
    "build:prod:ru": "ng build --configuration=production-ru",
    "build:prod:es": "ng build --configuration=production-es",
    "build:prod:de": "ng build --configuration=production-de",
    "build:server:prod:en": "ng run mega-iq-ui:server:production-en",
    "build:server:prod:ru": "ng run mega-iq-ui:server:production-ru",
    "build:server:prod:es": "ng run mega-iq-ui:server:production-es",
    "build:server:prod:de": "ng run mega-iq-ui:server:production-de",
    "build:library": "ng build ngx-example-library",
    "build:client-and-server-bundles-en": "npm run build:prod:en && npm run build:server:prod:en",
    "build:client-and-server-bundles-es": "npm run build:prod:es && npm run build:server:prod:es",
    "build:client-and-server-bundles-ru": "npm run build:prod:ru && npm run build:server:prod:ru",
    "build:client-and-server-bundles-de": "npm run build:prod:de && npm run build:server:prod:de",
    "build:ssr:en": "npm run build:client-and-server-bundles-en",
    "build:ssr:ru": "npm run build:client-and-server-bundles-ru",
    "build:ssr:es": "npm run build:client-and-server-bundles-es",
    "build:ssr:de": "npm run build:client-and-server-bundles-de",
    "serve:ssr_bak": "node dist/server",
    "dev:ssr": "ng run mega-iq-ui:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run mega-iq-ui:server:production",
    "prerender": "ng run mega-iq-ui:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "^11.1.0",
    "@angular/compiler": "^11.1.0",
    "@angular/core": "^11.1.0",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "^11.1.0",
    "@angular/localize": "^11.1.0",
    "@angular/platform-browser": "^11.1.0",
    "@angular/platform-browser-dynamic": "^11.1.0",
    "@angular/platform-server": "^11.1.0",
    "@angular/router": "^11.1.0",
    "@fortawesome/angular-fontawesome": "^0.8.1",
    "@fortawesome/fontawesome-common-types": "^0.2.34",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@nguniversal/express-engine": "^11.1.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "@types/chart.js": "^2.9.30",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "core-js": "^3.8.3",
    "express": "^4.17.1",
    "firebase": "^8.2.4",
    "jquery": "^3.5.1",
    "localstorage-polyfill": "^1.0.1",
    "ng-lazyload-image": "^9.1.0",
    "ngx-infinite-scroll": "^9.1.0",
    "ngx-owl-carousel": "^2.0.7",
    "ngx-sharebuttons": "^8.0.5",
    "node-sass": "^4.14.1",
    "popper": "^1.0.1",
    "rxjs": "^6.6.3",
    "sitemap": "^6.3.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.1",
    "@angular/cli": "^11.1.1",
    "@angular/compiler-cli": "^11.1.0",
    "@angular/language-service": "^11.1.0",
    "@nguniversal/builders": "^11.1.0",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.3",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-loader": "^8.0.14",
    "tslint": "^6.1.3",
    "typescript": "~4.1.3",
    "webpack-cli": "^3.3.12"
  }
}
