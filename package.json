{
  "name": "mega-iq-ui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n mega-iq-ui --i18n-format xlf2 --output-path i18n --i18n-locale en &&  node node_modules/.bin/ngx-extractor -i \"src/**/*.ts\" -f xlf2 -o \"src/i18n/messages.xlf\" && ng run mega-iq-ui:xliffmerge",
    "start-de": "ng serve --configuration=de",
    "start-ru": "ng serve --configuration=ru",
    "start-es": "ng serve --configuration=es",
    "build:prod:en": "ng build --configuration=production-en",
    "build:prod:ru": "ng build --configuration=production-ru",
    "build:prod:es": "ng build --configuration=production-es",
    "build:server:prod:en": "ng run mega-iq-ui:server:production-en",
    "build:server:prod:ru": "ng run mega-iq-ui:server:production-ru",
    "build:server:prod:es": "ng run mega-iq-ui:server:production-es",
    "build:library": "ng build ngx-example-library",
    "build:client-and-server-bundles-en": "npm run build:prod:en && npm run build:server:prod:en",
    "build:client-and-server-bundles-es": "npm run build:prod:es && npm run build:server:prod:es",
    "build:client-and-server-bundles-ru": "npm run build:prod:ru && npm run build:server:prod:ru",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr:en": "npm run build:client-and-server-bundles-en && npm run webpack:server",
    "build:ssr:ru": "npm run build:client-and-server-bundles-ru && npm run webpack:server",
    "build:ssr:es": "npm run build:client-and-server-bundles-es && npm run webpack:server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^8.2.2",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/platform-server": "^8.2.2",
    "@angular/router": "^8.2.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.9",
    "express": "^4.15.2",
    "feather-icons": "^4.24.0",
    "firebase": "^5.11.1",
    "jquery": "^3.4.1",
    "localstorage-ponyfill": "^1.0.2",
    "ngx-infinite-scroll": "^8.0.0",
    "ngx-owl-carousel": "^2.0.7",
    "node-sass": "^4.12.0",
    "popper": "^1.0.1",
    "rxjs": "^6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.802.2",
    "@angular/cli": "^8.2.2",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.3",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-loader": "^5.2.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.1.0"
  }
}
